# Example environment variables for the Redis pipeline
# Copy this file to .env for local development

# Redis configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
# For hosted Redis services, you can use REDIS_URL instead
# REDIS_URL=redis://username:password@host:port

# API configuration
API_BASE_URL=http://localhost:8000

# Dashboard configuration
HOST=0.0.0.0
PORT=8080
